---
import Layout from "../layouts/Layout.astro";
import { Image } from "astro:assets";
import type { ImageMetadata } from "astro";

// 1) Load all matching files from src/assets at build time.
//    Each entry value is Astro ImageMetadata (used by <Image src={...} />).
const imageModules = import.meta.glob<ImageMetadata>("../assets/*.{jpg,jpeg,png,webp,avif}", {
  eager: true,
  import: "default",
});

// 2) Helper: convert a filename (e.g. "cs-1.jpg") into its loaded asset metadata.
const getAsset = (fileName: string): ImageMetadata => {
  const asset = Object.entries(imageModules).find(([filePath]) =>
    filePath.endsWith(`/${fileName}`),
  )?.[1];

  if (!asset) {
    throw new Error(`Missing image asset: ${fileName}`);
  }

  return asset;
};

const heroBackground = getAsset("ric-2.jpg");

// 3) Define gallery data using filenames + alt text, then map to actual image sources.
const galleryImages = [
  { fileName: "cs-1.jpg", alt: "Photo 1" },
  { fileName: "cyn-1.jpg", alt: "Photo 2" },
  { fileName: "ej-6.jpg", alt: "Photo 3" },
  { fileName: "k-1.jpg", alt: "Photo 4" },
  { fileName: "kel-1.jpg", alt: "Photo 5" },
  { fileName: "nev-2.jpg", alt: "Photo 7" },
  { fileName: "ric-1.jpg", alt: "Photo 8" },
  { fileName: "har-1.jpg", alt: "Photo 9" },
  { fileName: "ej-4.jpg", alt: "Photo 12" },
  { fileName: "coop-1.jpg", alt: "Photo 13" },
  { fileName: "jd-1.jpg", alt: "Photo 14" },
  { fileName: "fj-1.jpg", alt: "Photo 15" },
  { fileName: "hw-1.jpg", alt: "Photo 16" },
  { fileName: "hw-2.jpg", alt: "Photo 17" },
  { fileName: "bike.jpg", alt: "Photo 18" },
].map(({ fileName, alt }) => ({
  src: getAsset(fileName),
  alt,
}));
